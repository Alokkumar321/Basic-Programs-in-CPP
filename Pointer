#include <stdio.h>

int main()
{
    int var = 789;
    int *ptr2;
    int **ptr1;
    ptr2 = &var;
    ptr1 = &ptr2;
    printf("Var =%d\n",var);
    printf("*ptr2 = %d\n",*ptr2);
    printf("ptr2 = %p\n",ptr2);
    printf("*ptr1 = %p\n",*ptr1);
    printf("**ptr1 = %d\n",**ptr1);
    printf("ptr1 = %p",ptr1);
    return 0;
}
