#include<iostream>
using namespace std;

class complex{
    int a,b,c;
    public:
    complex(void);
    void printNumber(){
        cout<<"Area of the triangle is = "<<a+b+c<<endl;
    }
};
complex :: complex(void){
    a=10;
    b=20;
    c=30;
}
int main(){
    complex c1;
    c1.printNumber();
    return 0;
}
