#include<stdio.h>
int main()
{
    int num1,num2,min,HCF;
    printf("Enter two numbers to get their HCF = ");
    scanf("%d%d",&num1,&num2);
    
    min = num1<num2 ? num1:num2;
    
    for(int i = 1;i<=min;i++)
    {
        {
            if(num1%i==0 && num2%i==0)
            {
                HCF = i;
            }
        }
    }
    printf("HCF of %d & %d is %d",num1,num2,HCF);
    return 0;
}
