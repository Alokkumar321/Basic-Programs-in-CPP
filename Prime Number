#include<stdio.h>
int main()
{
    int x,y,count=0;
    printf("Enter a number to check if it's a prime number or not = ");
    scanf("%d",&x);
    for(int i=1;i<=x;i++)
    {
        if(x%i==0)
        {
            count++;
        }
    }
    if(count == 2)
        printf("It is a prime number");
    else
       printf("It is not a prime number");
    return 0;
}
