#include<stdio.h>
int main()
{
    int a,b,i,max;
    printf("Enter to numbers to find their LCM = ");
    scanf("%d%d",&a,&b);
    max = a>b ? a:b;
    for(i=max;i<=a*b;i++)
    {
        if(i%a==0 && i%b==0)
        {
        break;
        }
    }
    printf("LCM of %d & %d is %d",a,b,i);
    return 0;
    
}
